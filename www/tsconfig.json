{
  "compilerOptions": {
    "target": "ES6",
    "useDefineForClassFields": false,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2016"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ES6",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true
  }
}
